#include <LiquidCrystal.h>

LiquidCrystal lcd(12, 11, 5, 4, 3, 2); int tempDelay=2000; int humidDelay=2000; void setup() {

lcd.begin(16,2); lcd.print("Hello!");
}

void loop() {
lcd.clear();
lcd.print("Temp:"); lcd.print(" C"); delay(tempDelay);

lcd.clear();
lcd.print("Humidity:"); lcd.print(" %"); delay(humidDelay);

lcd.clear();
}
